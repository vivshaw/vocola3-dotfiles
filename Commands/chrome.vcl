#Voice commands for Chrome with Vimium

#Link navigation
links = {f};

#History navigation
back = {Alt+Left};
forward = {Alt + Right};
Refresh = {Ctrl+r};

#Tab manipulation
next tab = {g}{t};
last tab = {g}{T};
zap it  = {Ctrl+w};

#Navigate in page
page top = {g}{g};

#Next/last
next page = "]" "]";
last page = "[" "[";

#Search
find = "/";